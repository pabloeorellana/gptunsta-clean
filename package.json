{
  "name": "nutrismart-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "dev:frontend": "vite",
    "dev:backend": "nodemon -r dotenv/config server/server.js",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "build": "rimraf dist && vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "create:user": "node -r dotenv/config server/scripts/createUser.js",
    "create:user:prod": "cross-env DOTENV_CONFIG_PATH=.env.production node -r dotenv/config server/scripts/createUser.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/list": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/x-date-pickers": "^7.9.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "luxon": "^3.7.2",
    "material-react-table": "^3.2.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.1",
    "nodemailer": "^7.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "react-social-login-buttons": "^4.1.1",
    "recharts": "^2.15.3",
    "tippy.js": "^6.3.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/node": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "cross-env": "^10.0.0",
    "cypress": "^15.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "overrides": {
    "date-fns": "^2.30.0"
  }
}
